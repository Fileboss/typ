<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import org.typ.controller.NoSpacetextField?>
<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="137.0" prefWidth="600.0" style="-fx-background-color: #072745;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <NoSpacetextField fx:id="textInput" onKeyPressed="#onSpacePressed" style="-fx-background-color: #0c3964; -fx-text-fill: #c5b028;">
               <VBox.margin>
                  <Insets bottom="20.0" left="40.0" right="40.0" top="5.0" />
               </VBox.margin>
               <font>
                  <Font name="DejaVu Sans Mono" size="18.0" />
               </font>
            </NoSpacetextField>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #072745;">
               <children>
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onClickExitButton" style="-fx-background-color: #23527e;" text="Quit" textAlignment="CENTER" textFill="#c5b028">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                     <font>
                        <Font name="DejaVu Sans Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="replayButton" mnemonicParsing="false" onAction="#onClickReplayButton" style="-fx-background-color: #23527e;" text="Replay" textFill="#c5b028">
                     <HBox.margin>
                        <Insets left="100.0" right="50.0" />
                     </HBox.margin>
                     <font>
                        <Font name="DejaVu Sans Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
